
@{
    Layout = null;
}
@model  List<Dictionary.Models.GetWordsByUserId_Result>

@foreach (var item in Model)
{
    <a href="javascript:void(0)" class="word_item p_flex_start" data-id-word="@Html.DisplayFor(modelItem => item.WordId)">
        <span class="word_text">@Html.DisplayFor(modelItem => item.sWord)</span>
        <span class="word_type"> Mô tả  : @Html.DisplayFor(modelItem => item.sDefinition)</span>
        <span class="time_search">Thời điểm tìm kiếm: @Html.DisplayFor(modelItem => item.dDatetime)</span>
        <span class="p_remove"></span>
    </a>
}
